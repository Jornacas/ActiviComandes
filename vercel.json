{
  "version": 2,
  "builds": [
    {
      "src": "frontend/package.json",
      "use": "@vercel/next",
      "config": {
        "distDir": "frontend/.next"
      }
    },
    {
      "src": "app-mobil/package.json", 
      "use": "@vercel/next",
      "config": {
        "distDir": "app-mobil/.next"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "frontend/$1"
    }
  ]
}
